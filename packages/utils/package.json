{
  "name": "@pkg/utils",
  "version": "0.0.0",
  "private": true,
  "description": "General utilities for Node and the browser",
  "scripts": {
    "lint": "eslint \"./**/*.{ts,tsx,js,mjs,cjs}\"",
    "lint:fix": "eslint \"./**/*.{ts,tsx,js,mjs,cjs}\" --fix",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "pnpm test:coverage && open coverage/index.html && vitest --ui --open",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "next": "^13.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.30.1",
    "eslint-plugin-vitest": "^0.1.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  }
}
