{
  "name": "@pkg/mdx",
  "version": "0.0.0",
  "private": true,
  "description": "Better MDX support for NextJS, with FrontMatter YAML parsing, full reload, and more.",
  "scripts": {
    "postinstall": "node lib/next-plugin/init.js",
    "lint": "eslint \"./**/*.{ts,tsx,js,mjs,cjs}\"",
    "lint:fix": "eslint \"./**/*.{ts,tsx,js,mjs,cjs}\" --fix",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "pnpm test:coverage && open coverage/index.html && vitest --ui --open",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "chokidar": "^3.5.3",
    "glob": "^10.2.7",
    "gray-matter": "^4.0.3",
    "next": "13.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "typescript": "^5.1.3"
  }
}
