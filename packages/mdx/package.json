{
  "name": "@pkg/mdx",
  "version": "0.0.0",
  "private": true,
  "description": "Better MDX support for NextJS, with FrontMatter YAML parsing, full reload, and more.",
  "scripts": {
    "postinstall": "node src/next-plugin/init.js",
    "lint": "eslint \"./**/*.{ts,tsx,js,mjs,cjs}\"",
    "lint:fix": "eslint \"./**/*.{ts,tsx,js,mjs,cjs}\" --fix",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "pnpm test:coverage && open coverage/index.html && vitest --ui --open",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "chokidar": "^3.5.3",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "glob": "^9.3.4",
    "gray-matter": "^4.0.3",
    "next": "^13.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.6",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.29.8",
    "eslint-plugin-vitest": "^0.1.1",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }
}
