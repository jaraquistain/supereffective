{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "id": "gg.supereffective.app",
  "name": "SuperEffective",
  "description": "The most complete Living Dex tracker and Pokémon game companion.",
  "short_name": "SuperEffective",
  "start_url": "/",
  "theme_color": "#000000",
  "background_color": "#000000",
  "display": "standalone",
  "lang": "en",
  "orientation": "portrait",
  "display_override": ["standalone", "window-controls-overlay", "minimal-ui", "browser"],
  "icons": [
    {
      "src": "/assets/favicon/android-chrome-192x192.png?v=2024-002",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/assets/favicon/android-chrome-384x384.png?v=2024-002",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "/assets/favicon/android-chrome-512x512.png?v=2024-002",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/sv3-001.jpg",
      "sizes": "1280x720",
      "type": "image/jpg",
      "platform": "wide"
    }
  ],
  "shortcuts": [
    {
      "name": "Pokédex",
      "short_name": "Pokédex",
      "description": "Pokédex page",
      "url": "/apps/pokedex"
    },
    {
      "name": "Living Dexes",
      "short_name": "LivingDex",
      "description": "Living Dexes page",
      "url": "/apps/livingdex"
    },
    {
      "name": "Missing Pokémon",
      "short_name": "Missing",
      "description": "Missing Pokémon page",
      "url": "/apps/livingdex/missing"
    }
  ],
  "launch_handler": {
    "client_mode": "auto"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+supereffective",
      "url": "/%s"
    }
  ],
  "related_applications": [],
  "categories": ["games", "utilities"]
}
