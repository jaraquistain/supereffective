{
  "name": "supereffective",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.com:itsjavi/supereffective.git",
  "license": "proprietary",
  "author": "Javi Aguilar https://itsjavi.com",
  "scripts": {
    "build": "turbo run build",
    "dev": "pnpm install && turbo run dev --no-deps --no-cache --parallel --concurrency 10",
    "dev:docker": "docker-compose up -d dev && sleep 10 && pnpm open",
    "docker": "docker-compose run --rm dev /bin/zsh",
    "format": "pnpm format:packages && pnpm format:code && pnpm lint:fix",
    "format:code": "prettier --write \"**/*.{cjs,js,jsx,ts,tsx,json,yml,yaml,css,scss,md,mdx}\"",
    "format:packages": "sort-package-json \"package.json\" \"apps/*/package.json\"",
    "postinstall": "if [ \"$NODE_ENV\" != \"production\" ]; then make postinstall-dev; fi",
    "lint": "eslint --cache \"./apps/*/src/**/*.{ts,tsx}\" --max-warnings=25",
    "lint:fix": "eslint --fix \"./apps/*/src/**/*.{ts,tsx}\"",
    "open": "open http://localhost:3001",
    "pretty": "pnpm format",
    "update:data": "pnpm -r data:sync",
    "update:latest": "pnpm update -r --latest"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.8.8",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "sort-package-json": "^2.4.1",
    "turbo": "^1.8.8",
    "typescript": "^4.9.5"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=18.5.0 <19.0.0"
  }
}
