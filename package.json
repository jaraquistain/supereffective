{
  "name": "supereffective",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.com:itsjavi/supereffective.git",
  "license": "proprietary",
  "author": "Javi Aguilar https://itsjavi.com",
  "scripts": {
    "build": "turbo run build && pnpm format:code",
    "build:vercel": "pnpm pkg:database migrate && turbo run build",
    "dev": "pnpm install --ignore-scripts && turbo run dev --no-deps --no-cache --parallel --concurrency 20",
    "dev:docker": "docker-compose up -d",
    "docker": "docker-compose run --rm dev",
    "docker:sh": "docker-compose run --rm dev /bin/zsh",
    "dprisma": "pnpm docker pnpm pkg:database prisma",
    "format": "pnpm format:packages && pnpm format:code && pnpm lint:fix",
    "format:code": "prettier --write \"**/*.{cjs,mjs,js,ts,tsx,json,yml,yaml,css,scss,md,mdx}\"",
    "format:packages": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "postinstall": "make postinstall",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "open": "open http://localhost:3001 && open http://localhost:3002 && open http://localhost:1080",
    "pkg:auth": "pnpm --filter '@pkg/auth'",
    "pkg:database": "pnpm --filter '@pkg/database'",
    "pkg:ui": "pnpm --filter '@pkg/ui'",
    "pkg:website": "pnpm --filter '@app/website'",
    "pretty": "pnpm format",
    "test": "turbo run --concurrency 1 test",
    "test:coverage": "turbo run --concurrency 1 test:coverage",
    "test:watch": "turbo run test:watch",
    "update:packages": "pnpm update -r --latest"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.9.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-vitest": "^0.1.5",
    "husky": "^8.0.3",
    "next": "^13.4.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sort-package-json": "^2.4.1",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.31.0",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  },
  "packageManager": "pnpm@8.4.0",
  "engines": {
    "node": ">=18.5.0 <19.0.0"
  }
}
